# editorconfig.org
root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.json]
indent_size = 2

[*.scss]
indent_size = 2

[.*rc]
indent_size = 2

[*.yml]
indent_size = 2

[*.md]
trim_trailing_whitespace = false

[*.min.js]
insert_final_newline = false

[*.meta.js]
insert_final_newline = false

[*.css]
insert_final_newline = false
